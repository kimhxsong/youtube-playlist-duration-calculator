{
  "manifest_version": 3,
  "name": "YouTube Playlist Total Time",
  "version": "1.0",
  "description": "YouTube 플레이리스트의 전체 시간과 진행률을 계산하여 콘솔에 출력합니다.",
  "permissions": ["activeTab"],
  "content_scripts": [{
    "matches": ["*://*.youtube.com/*"],
    "js": ["content.js"],
    "run_at": "document_end"
  }]
}